<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<formatVersion>4</formatVersion>
	<view name="system">
	
		<image name="background" extra="static">
			<path>${backgroundImage}</path>
		</image>
		
		<video name="videobox" extra="true">
			<visible>false</visible>
			<zIndex>1</zIndex>
		</video>
		
		<image name="CenterFade" extra="true">
			<tile>false</tile>
			<size>1 0.5</size>
			<pos>0.5 0.5</pos>
			<origin>0.5 0.5</origin>
			<path>./../centerfade.png</path>
			<color>${centerFadeColor}</color>
			<zIndex>2</zIndex>
		</image>
		
	</view>
	
</theme>
